    <script language="javascript" src="http://www.mrhaki.com/js/syntax/shBrushXml.js"></script>
    <script language="javascript" src="http://www.mrhaki.com/js/syntax/shBrushJScript.js"></script>
    <script language="javascript" src="http://www.mrhaki.com/js/syntax/shBrushGroovy.js"></script>
